<?xml version="1.0" encoding="UTF-8"?>
<CLISH_MODULE xmlns="http://clish.sourceforge.net/XMLSchema" 
			  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
			  xsi:schemaLocation="http://clish.sourceforge.net/XMLSchema
              http://clish.sourceforge.net/XMLSchema/clish.xsd">
<!--===================================================-->
    <VIEW name="net-dhcp-dmz-config"
        prompt="net-config[dhcp-dmz-ip]> "> 
    <COMMAND name="save" help="Save DHCP Reserved IPs configuration changes.">
	<ACTION>lua clishCfgObjExec("dmzDhcpfixedIpAddress")</ACTION>
    </COMMAND>
    <COMMAND name="exit" 
	     help="Save DHCP Reserved IPs configuration changes and exit current mode."
	     view="root-view" viewid="">
        <ACTION>lua clishCfgObjExec("dmzDhcpfixedIpAddress")</ACTION>
    </COMMAND>
    <COMMAND name="cancel" help="Roll back DHCP Reserved IPs configuration changes.">
        <ACTION>lua clishCfgObjReset()</ACTION>
    </COMMAND>
    <COMMAND name="ip_address" help="Set IP Address to be reserved">
	<PARAM name="dhcp_reserved_ip" 
        help="IP Address to be reserved"
	       ptype="IP_ADDR"/>
	<ACTION>lua clishCfgObjAdd ("dmzDhcpfixedIpAddress", "DhcpfixedIpAddress.IpAddr",
                                   "${dhcp_reserved_ip}")
	</ACTION>
    </COMMAND>
</VIEW>
    <!--=======================================================-->
    <COMMAND name="net dmz dhcp" help="DHCP setup."/>
    <COMMAND name="net dmz dhcp reserved_ip" help="DHCP Reserved IPs setup."/>
    <!--=======================================================-->
    <COMMAND name="net dmz dhcp reserved_ip configure"
             help="DHCP Reserved IPs add/edit mode."
             view="net-dhcp-dmz-config" viewid="">
             <PARAM name="mac_address"
                 help="Reserved Mac address you want to add/edit."
                 ptype="MAC_ADDR_T"/>
            <ACTION>lua clishCfgObjNew ("dmzDhcpfixedIpAddress", 
                                       dmzDhcpReservedIPsCfgInit, 
                                       dmzDhcpReservedIPsCfgSave,
                                       dmzDhcpReservedIPsCfgInputVal,
                                       "${mac_address}")
	    </ACTION>
    </COMMAND>
    <!--=======================================================-->
    <COMMAND name="net dmz dhcp reserved_ip delete"
             help="Delete a specific reserved ip entry.">
	<PARAM name="mac_address"
                 help="Reserved Mac address you want to delete."
                 ptype="MAC_ADDR_T"/>
            <ACTION>lua dmzDhcpReservedIPsCfgDel("${mac_address}") 
	    </ACTION>
    </COMMAND>
    <!--=======================================================-->
    
</CLISH_MODULE>
