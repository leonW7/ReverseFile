<VIEW name="sslvpn-portal-layout-configure-add" prompt="[portal-settings]> "> 
    <COMMAND name="save" help="Save portal settings">
	     <ACTION>lua clishCfgObjExec("SSLVPNPortalLayout")
	     </ACTION>
    </COMMAND>
    <COMMAND name="exit" 
	     help="Save portal settings and exit current mode" 
	     view="root-view" viewid="">
		 <ACTION>lua clishCfgObjExec("SSLVPNPortalLayout")
		 </ACTION>
    </COMMAND>
    <COMMAND name="cancel" help="Roll back portal settings changes">
         <ACTION>lua clishCfgObjReset()</ACTION>
    </COMMAND>
	<COMMAND name="portal_name"
             help="Specify the portal name">
             <PARAM name="portal_name"
                    help="Specify the portal name"
                    ptype="STRING_ALPHA_NUM_31"/>
             <ACTION>lua clishCfgObjAdd ("SSLVPNPortalLayout", "SSLVPNPortalLayout.PortalLayoutName", "${portal_name}")
             </ACTION>
   </COMMAND>
   <COMMAND name="profile_name"
             help="Specify the profile name">
             <PARAM name="profile_name"
                    help="Specify the profile name"
                    ptype="STRING_ALPHA_NUM"/>
             <ACTION>lua clishCfgObjAdd ("SSLVPNPortalLayout", "SSLVPNPortalLayout.profileName", "${profile_name}")
             </ACTION>
    </COMMAND>     
	<COMMAND name="portal_title"
             help="Specify the portal title">
             <PARAM name="portal_title"
                    help="Specify the portal title"
                    ptype="STRING64"/>
             <ACTION>lua clishCfgObjAdd ("SSLVPNPortalLayout", "SSLVPNPortalLayout.PortalSiteTitle", "${portal_title}")
             </ACTION>
    </COMMAND>
	<COMMAND name="banner_title"
             help="Specify the banner title">
             <PARAM name="banner_title"
                    help="Specify the banner title"
                    ptype="STRING64_ACCEPT_SPACE"/>
             <ACTION>lua clishCfgObjAdd ("SSLVPNPortalLayout", "SSLVPNPortalLayout.BannerTitle", "${banner_title}")
             </ACTION>
    </COMMAND>
	<COMMAND name="banner_message"
             help="Specify the banner message">
             <PARAM name="banner_message"
                    help="Specify the banner message"
                    ptype="STRING_ACCEPT_SPACE"/>
             <ACTION>lua clishCfgObjAdd ("SSLVPNPortalLayout", "SSLVPNPortalLayout.BannerMessage", "${banner_message}")
             </ACTION>
    </COMMAND>
	<COMMAND name="display_banner"
             help="Specify whether the banner message should be displayed">
             <PARAM name="display_banner"
                    help="Specify whether the banner message should be displayed"
                    ptype="BOOL"/>
             <ACTION>lua clishCfgObjAdd ("SSLVPNPortalLayout", "SSLVPNPortalLayout.DisplayBanner", "${display_banner}")
             </ACTION>
    </COMMAND>
	<COMMAND name="enable_httpmetatags"
             help="Specify whether the http meta tags should be enabled">
             <PARAM name="enable_httpmetatags"
                    help="Specify whether the http meta tags should be enabled"
                    ptype="BOOL"/>
             <ACTION>lua clishCfgObjAdd ("SSLVPNPortalLayout", "SSLVPNPortalLayout.EnableHTTPMetaTags", "${enable_httpmetatags}")
             </ACTION>
    </COMMAND>
	<COMMAND name="enable_activexwebcachecleaner"
             help="Specify whether the activex web cache cleaner should be enabled">
             <PARAM name="enable_activexwebcachecleaner"
                    help="Specify whether the activex web cache cleaner should be enabled"
                    ptype="BOOL"/>
             <ACTION>lua clishCfgObjAdd ("SSLVPNPortalLayout", "SSLVPNPortalLayout.EnableActiveXWebCacheCleaner", "${enable_activexwebcachecleaner}")
             </ACTION>
    </COMMAND>
	<COMMAND name="enable_vpntunnel"
             help="Specify whether the vpn tunnel should be enabled">
             <PARAM name="enable_vpntunnel"
                    help="Specify whether the vpn tunnel should be enabled"
                    ptype="BOOL"/>
             <ACTION>lua clishCfgObjAdd ("SSLVPNPortalLayout", "SSLVPNPortalLayout.EnableVPNTunnel", "${enable_vpntunnel}")
             </ACTION>
    </COMMAND>
	<COMMAND name="enable_portforwarding"
             help="Specify whether the port forwarding should be enabled">
             <PARAM name="enable_portforwarding"
                    help="Specify whether the port forwarding should be enabled"
                    ptype="BOOL"/>
             <ACTION>lua clishCfgObjAdd ("SSLVPNPortalLayout", "SSLVPNPortalLayout.EnablePortForwarding", "${enable_portforwarding}")
             </ACTION>
    </COMMAND>
    <COMMAND name="sslAuthType"
        help="Authentication method for the portal.">
        <PARAM name="sslAuthType"
            help="Select a authentication type."
            ptype="AUTH_TYPE"/>
        <ACTION>lua clishCfgObjAdd ("SSLVPNPortalLayout", "SSLVPNPortalLayout.sslAuthType", "${sslAuthType}")
        </ACTION>
    </COMMAND>
    <COMMAND name="SSL_GROUP"
        help="group name for the portal(only for local authentication).">
        <PARAM name="sslGroupName"
            help="Select a group"
            ptype="STRING32"/>
        <ACTION>lua clishCfgObjAdd ("SSLVPNPortalLayout", "SSLVPNPortalLayout.sslGroupName", "${sslGroupName}")
        </ACTION>
    </COMMAND>
</VIEW>
