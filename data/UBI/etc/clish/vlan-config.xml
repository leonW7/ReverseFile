<!--=======================================================-->
    <COMMAND name="net vlan config" help="vlan configuration ."/>    
    <!--=======================================================-->
    <COMMAND name="net vlan config add"
             help="Add a vlan."
             view="net-vlan-config" viewid="">
    <PARAM name="vlan_id" 
           help="Give id of the vlan to be created. "
           ptype="VLAN_ID_RANGE"/>
               <ACTION>lua clishCfgObjNew ("vlan",vlanCfgInit, vlanCfgSave,vlanCfgInputVal,"${vlan_id}","add")
                    </ACTION>
    </COMMAND>
    <COMMAND name="net vlan config edit"
             help="Edit a configured vlan."
             view="net-vlan-config" viewid="">
    <PARAM name="vlan_Id" 
           help="Enter Id of vlan to be edited,use 'show net vlan configuration' for available vlan list"
           ptype="VLANID_RANGE"/>
           	<ACTION>lua clishCfgObjNew ("vlan",vlanCfgInit, vlanCfgSave,vlanCfgInputVal,"${vlan_Id}","edit")
                    </ACTION>
                  
    </COMMAND>
    <COMMAND name="net vlan config delete"
             help="Delete a vlan.">
    <PARAM name="VlanId" 
           help="Enter vlan ID of vlan to be deleted"
           ptype="VLAN_ID_RANGE"/>
          	<ACTION>lua clishCmdExec (vlanAvailableDelete,"${VlanId}")
                   </ACTION>
    </COMMAND>
    <COMMAND name="net vlan config enable"
             help="Enable vlan configuration.">
              	<ACTION>lua clishCmdExec (vlanConfigEnable)
                   </ACTION>
    </COMMAND>
    <COMMAND name="net vlan config disable"
             help="Disable vlan configuration.">
              	<ACTION>lua clishCmdExec (vlanConfigDisable)
                   </ACTION>
    </COMMAND>
</CLISH_MODULE>
