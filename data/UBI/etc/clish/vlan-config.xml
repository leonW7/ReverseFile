<VIEW name="net-vlan-ipv6-config" prompt="net-config[vlan-ipv6]> "> 
    <COMMAND name="save" help="Save VLAN configuration changes.">
	     <ACTION>lua clishCfgObjExec("vlanv6")
	     </ACTION>
    </COMMAND>
    <COMMAND name="exit" 
	     help="Save VLAN configuration changes and exit current mode." 
	     view="root-view" viewid="">
	     <ACTION>lua clishCfgObjExec("vlanv6")</ACTION>
    </COMMAND>
    <COMMAND name="cancel" help="Roll back VLAN configuration changes.">
	     <ACTION>lua clishCfgObjReset()</ACTION>
     </COMMAND>
    <COMMAND name="IPv6_enable" help="Set IPv6 Vlan status">
	<PARAM name="ip_address" 
	       help="Ipv6 Vlan status"
	       ptype="CHOICE_INT"/>
	<ACTION>lua clishCfgObjAdd ("vlanv6", "ifStatic.isEnabled", 
		    "${ip_address}")
	</ACTION>
    </COMMAND>
    <COMMAND name="static" help="Set system VLAN Settings."/>
    <COMMAND name="static address" help="Set system VLAN IP address.">
	<PARAM name="ip_address" 
	       help="VLAN IP address."
	       ptype="IPV6_ADDR"/>
	<ACTION>lua clishCfgObjAdd ("vlanv6", "ifStatic.StaticIp", 
		    "${ip_address}")
	</ACTION>
    </COMMAND>
    <COMMAND name="static prefix_value" help="Prefix length">
	<PARAM name="prefix_length" 
	       help="Prefix Length"
	       ptype="UINT"/>
	<ACTION>lua clishCfgObjAdd ("vlanv6", "ifStatic.PrefixLength", 
		    "${prefix_length}")
	</ACTION>
    </COMMAND>
    <COMMAND name="dhcp" help="Set system VLAN Settings."/>
    <COMMAND name="dhcp server_enable" help="Set dhcpv6 server status">
	<PARAM name="ip_address" 
	       help="Prefix Length"
	       ptype="CHOICE_INT"/>
	<ACTION>lua clishCfgObjAdd ("vlanv6", "dhcpv6s.isEnabled", 
		    "${ip_address}")
	</ACTION>
    </COMMAND>
    <COMMAND name="dhcp mode" help="DHCPv6 Mode">
	<PARAM name="ip_address" 
	       help="DHCPv6 Mode"
	       ptype="DHCPV6_MODE"/>
	<ACTION>lua clishCfgObjAdd ("vlanv6", "dhcpv6s.statelessMode", 
		    "${ip_address}")
	</ACTION>
    </COMMAND>
    <COMMAND name="dhcp domain_name" help="dhcp server domain name">
	<PARAM name="ip_address" 
	       help="dhcpv6 domain server name"
	       ptype="DHCP_DOMAIN"/>
	<ACTION>lua clishCfgObjAdd ("vlanv6", "dhcpv6s.domainName", 
		    "${ip_address}")
	</ACTION>
    </COMMAND>
    <COMMAND name="dhcp server_preference" help="server preference number">
	<PARAM name="ip_address" 
	       help="dhcp server preference number"
	       ptype="UINT"/>
	<ACTION>lua clishCfgObjAdd ("vlanv6", "dhcpv6s.serverPreference", 
		    "${ip_address}")
	</ACTION>
    </COMMAND>
    <COMMAND name="dhcp dns_type" help="dns server type">
	<PARAM name="ip_address" 
	       help="DNS Server Type 1(useDnsProxy) 2(useDnsFromISP) 3(useBelow)"
	       ptype="DHCPV6S_DNS_T"/>
	<ACTION>lua clishCfgObjAdd ("vlanv6", "dhcpv6s.useDNSServersFrom", 
		    "${ip_address}")
	</ACTION>
    </COMMAND>
    <COMMAND name="dhcp primary_dns" help="primary dns server ">
	<PARAM name="ip_address" 
	       help="Primary dns server "
	       ptype="IPV6_ADDR"/>
	<ACTION>lua clishCfgObjAdd ("vlanv6", "dhcpv6s.primaryDNSServer", 
		    "${ip_address}")
	</ACTION>
    </COMMAND>
    <COMMAND name="dhcp secondary_dns" help="Secondary dns server">
	<PARAM name="ip_address" 
	       help="Secondary dns server"
	       ptype="IPV6_ADDR"/>
	<ACTION>lua clishCfgObjAdd ("vlanv6", "dhcpv6s.secondaryDNSServer", 
		    "${ip_address}")
	</ACTION>
    </COMMAND>
    <COMMAND name="dhcp rebind_time" help="Rebind time">
	<PARAM name="ip_address" 
	       help="rebind time"
	       ptype="DHCPV6S_REBIND_TIME"/>
	<ACTION>lua clishCfgObjAdd ("vlanv6", "dhcpv6s.leaseTime", 
		    "${ip_address}")
	</ACTION>
</COMMAND>
    <COMMAND name="dhcp Prefix_delegation" help="Prefix Delegation">
	<PARAM name="ip_address" 
	       help="Prefix Delegation Status"
	       ptype="CHOICE_INT"/>
	<ACTION>lua clishCfgObjAdd ("vlanv6", "dhcpv6s.prefixDelegation", 
		    "${ip_address}")
	</ACTION>
    </COMMAND>
</VIEW>

