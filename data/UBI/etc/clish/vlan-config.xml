<?xml version="1.0" encoding="UTF-8"?>
<CLISH_MODULE xmlns="http://clish.sourceforge.net/XMLSchema" 
			  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
			  xsi:schemaLocation="http://clish.sourceforge.net/XMLSchema
              http://clish.sourceforge.net/XMLSchema/clish.xsd">
<!--=======================================================-->
<VIEW name="net-vlan-config" prompt="net-config[vlanConfig]> "> 
    <COMMAND name="save" help="Save vlan configuration changes.">
	     <ACTION>lua clishCfgObjExec("vlan")
	     </ACTION>
    </COMMAND>
    <COMMAND name="exit" 
	     help="Save vlan changes and exit current mode." 
	     view="root-view" viewid="">
	     <ACTION>lua clishCfgObjExec("vlan")</ACTION>
    </COMMAND>
    <COMMAND name="cancel" help="Roll back configuration changes.">
	     <ACTION>lua clishCfgObjReset()</ACTION>
    </COMMAND>
       <COMMAND name="vlan-name"
             help="Name for vlan.">
             <PARAM name="vlanName"
                    help="Name for Vlan"
                    ptype="STRING_ALPHA_NUM"/>
             <ACTION>lua clishCfgObjAdd ("vlan","vlan.vlanName", "${vlanName}")              
             </ACTION>
    </COMMAND>
	<COMMAND name="inter-vlan-routing"
             help="Enable/Disable interVlan Routing .">
             <PARAM name="interVlan"
                    help="Enable/Disable"
                    ptype="CHOICE_INT"/>
             <ACTION>lua clishCfgObjAdd ("vlan","interVLANRouting.status", "${interVlan}")              
             </ACTION>
    </COMMAND>
       <COMMAND name="showCP-profiles" check_supported="Y"
             help="available captive portal profiles">
             <ACTION>lua clishCmdExec (captivePortalShowProfile)              
             </ACTION>
    </COMMAND>
    <COMMAND name="captive-portal-enable"
             help="Enable captive portal">
             <PARAM name="accessType"
                    help="Select accessType "
                    ptype="CHOICE_INT_CP"/>
             <ACTION>lua clishCfgObjAdd ("vlan","interVLANRouting.accessType", "${accessType}")              
             </ACTION>
    </COMMAND>
    <COMMAND name="captive-portal-user" help="accessType is Permanent User "/>
 	<COMMAND name="captive-portal-user auth-mode" help="authentication-server for user">
		<PARAM name="authMode"
            help=" authentication type"
            ptype="CP_AUTH_MODE"/>
         <ACTION>lua clishCfgObjAdd ("vlan", "CaptivePortalVLAN.authMode","${authMode}")
             </ACTION>
    </COMMAND>
       <COMMAND name="captive-portal-user auth-type" help="authentication-type for user">
	<PARAM name="authType"
            help=" authentication server type"
            ptype="CP_AUTH_TYPE"/>
         <ACTION>lua clishCfgObjAdd ("vlan", "CaptivePortalVLAN.authType","${authType}")
             </ACTION>
    </COMMAND>
           <COMMAND name="captive-portal-user CP-profile" help="captive portal profile for user">
		<PARAM name="perCPprofile"
            help=" captive protal Profile Id"
            ptype="CP_PROFILE"/>
         <ACTION>lua clishCfgObjAdd ("vlan", "CaptivePortalVLAN.profileId","${perCPprofile}")
             </ACTION>
     </COMMAND>
   
     <COMMAND name="captive-portal-user Redirect-Type" help="captive portal profile for user"> 
		<PARAM name="perredirect"
            help=" captive protal redirect type"
            ptype="REDIRECT_TYPE"/>
         <ACTION>lua clishCfgObjAdd ("vlan", "CaptivePortalVLAN.redirectType","${perredirect}")
             </ACTION>
     </COMMAND>

 </VIEW>

