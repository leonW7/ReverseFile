<VIEW name="MultiVlan-edit-config" prompt="MultiVlan-config[policy]> ">
<COMMAND name="save" help="Save Multi Vlan server configuration changes.">
        <ACTION>lua clishCfgObjExec("MultivlanSub")</ACTION>
    </COMMAND>
<COMMAND name="cancel" help="Roll back Multi Vlan server configuration changes.">
        <ACTION>lua clishCfgObjReset()</ACTION>
    </COMMAND>

<COMMAND name="exit" 
             help="Save Multivlan server configuration changes and exit current mode."
             view="root-view" viewid="">
        <ACTION>lua clishCfgObjExec("MultivlanSub")</ACTION>
</COMMAND>

<COMMAND name="ip-address"
             help="IP of given MultiVlan subnetVlan.">
     <PARAM name="addr_ip" 
            help="add IP."
            ptype="IP_ADDR"/>
         <ACTION>lua  clishCfgObjAdd("MultivlanSub","ifStatic.StaticIp","${addr_ip}")
         </ACTION>
     </COMMAND>

<COMMAND name="subnet-mask"
    help="Add subnet mask for Multivlan.">
     <PARAM name="mas_subnet" 
            help="Subnet mask for the vlan"
            ptype="IP_ADDR"/>
         <ACTION>lua
             clishCfgObjAdd("MultivlanSub","ifStatic.NetMask","${mas_subnet}")
         </ACTION>
     </COMMAND>

<COMMAND name="dhcp-mode"
    help="Select the DHCP Mode.">
     <PARAM name="dhcp_mod" 
            help="Subnet mask for the vlan"
            ptype="DHCPV4_MODE"/>
         <ACTION>lua clishCfgObjAdd("MultivlanSub","dhcp.dhcpEnabled","${dhcp_mod}")
         </ACTION>
    </COMMAND>

<COMMAND name="domain-name"
    help="domain name for vlan.">
     <PARAM name="dom_nam" 
            help="domain name for the vlan"
            ptype="DHCP_DOMAIN"/>
         <ACTION>lua clishCfgObjAdd("MultivlanSub","dhcp.domainName","${dom_nam}")
         </ACTION>
    </COMMAND>

<COMMAND name="start-ip"
    help="starting ip address of the dhcp mode.">
     <PARAM name="ip_start" 
            help="Starting ip for the vlan"
            ptype="IP_ADDR"/>
         <ACTION>lua clishCfgObjAdd("MultivlanSub","dhcp.startAddress","${ip_start}")
         </ACTION>
</COMMAND>

<COMMAND name="end-ip"
    help="ending ip of the Valn.">
     <PARAM name="ip_end" 
            help="ending ip for the vlan"
            ptype="IP_ADDR"/>
         <ACTION>lua
             clishCfgObjAdd("MultivlanSub","dhcp.endAddress","${ip_end}")
         </ACTION>
</COMMAND>

<COMMAND name="primary-dns"
    help="Primary dns for the vlan.">
     <PARAM name="dns_primary" 
            help="primary dns for the vlan"
            ptype="IP_ADDR"/>
         <ACTION>lua
             clishCfgObjAdd("MultivlanSub","dhcp.primaryDnsServer","${dns_primary}")
         </ACTION>
</COMMAND>

<COMMAND name="secondary-dns"
    help="Secondary dns for the vlan.">
     <PARAM name="dns_secondary" 
            help="secondary dns for the vlan"
            ptype="IP_ADDR"/>
         <ACTION>lua
             clishCfgObjAdd("MultivlanSub","dhcp.secondaryDnsServer","${dns_secondary}")
         </ACTION>
</COMMAND>

<COMMAND name="default_gw"
    help="Set default gateway.">
     <PARAM name="defaultgw" 
            help="Default Gateway."
            ptype="IP_ADDR"/>
         <ACTION>lua
             clishCfgObjAdd("MultivlanSub","dhcp.defaultGw", "${defaultgw}")
         </ACTION>
</COMMAND>

<COMMAND name="lease-time"
    help="Lease time for the vlan.">
     <PARAM name="time_lease" 
            help="lease time for the vlan"
            ptype="LEASETIME"/>
         <ACTION>lua
             clishCfgObjAdd("MultivlanSub","dhcp.leaseTime","${time_lease}")
         </ACTION>
</COMMAND>

<COMMAND name="relay-gateway"
    help="relay gateway for the vlan.">
     <PARAM name="relay_gat" 
            help="relay gateway for the vlan"
            ptype="IP_ADDR"/>
         <ACTION>lua
             clishCfgObjAdd("MultivlanSub","DhcpRelay.RelayGateway","${relay_gat}")
         </ACTION>
</COMMAND>


<COMMAND name="enable-dns-proxy"
    help="relay gateway for the vlan.">
     <PARAM name="enable_dns" 
            help="relay gateway for the vlan"
            ptype="CHOICE_INT"/>
         <ACTION>lua
             clishCfgObjAdd("MultivlanSub","dhcp.enableDnsProxy","${enable_dns}")
         </ACTION>
</COMMAND>
</VIEW>
