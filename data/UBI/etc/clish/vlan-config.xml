<VIEW name="net-port-vlan-config" prompt="net-port-config[portVlanConfig]> "> 
    <COMMAND name="save" help="Save vlan configuration changes.">
	     <ACTION>lua clishCfgObjExec("portVlan")
	     </ACTION>
    </COMMAND>
    <COMMAND name="exit" 
	     help="Save vlan changes and exit current mode." 
	     view="root-view" viewid="">
	     <ACTION>lua clishCfgObjExec("portVlan")</ACTION>
    </COMMAND>
    <COMMAND name="cancel" help="Roll back configuration changes.">
	     <ACTION>lua clishCfgObjReset()</ACTION>
    </COMMAND>
    <COMMAND name="mode"
             help="Port Vlan mode  .">
             <PARAM name="vlanMode"
                    help="vlan port mode"
                    ptype="PORT_VLAN_MODE"/>
             <ACTION>lua clishCfgObjAdd ("portVlan","VlanPortMgmt.PortVlanMode", "${vlanMode}")              
             </ACTION>
    </COMMAND>
         <COMMAND name="pvid"
             help="Port Vlan ID">
             <PARAM name="pvid"
                    help="Port vlan ID  "
                    ptype="VLANID_RANGE"/>
             <ACTION>lua clishCfgObjAdd ("portVlan","VlanPortMgmt.DefaultPVID", "${pvid}")              
             </ACTION>
     </COMMAND>
</VIEW>
