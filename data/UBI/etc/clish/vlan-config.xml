<VIEW name="net-vlan-membership-config" prompt="Vlan-config[membership]> ">
    <COMMAND name="save" help="Save Vlan Membership configuration changes.">
        <ACTION>lua clishCfgObjExec("vlanMembership")</ACTION>
     </COMMAND>
    <COMMAND name="cancel" help="Roll back vlan membership configuration changes.">
        <ACTION>lua clishCfgObjReset()</ACTION>
     </COMMAND>
    <COMMAND name="exit" 
             help="Save vlan membership configuration changes and exit current mode."
             view="root-view" viewid="">
        <ACTION>lua clishCfgObjExec("vlanMembership")</ACTION>
     </COMMAND>
    <COMMAND name="membership" 
             help="vlan membership.Give comma seperated VLAN ID values."/>
      <COMMAND name="membership add" 
             help="vlan membership.Give comma seperated VLAN ID values.">
             <PARAM name="vlanIds1"
                help="Give vlan Id comma seperated values."
            	ptype="STRING"/>
        <ACTION>lua clishCfgObjAdd ("vlanMembership","vlan.vlanIds1", "${vlanIds1}")
        </ACTION>
     </COMMAND>
      <COMMAND name="membership remove" 
             help="vlan membership.Give comma seperated VLAN ID values.">
             <PARAM name="vlanIds2"
                help="Give vlan Id comma seperated values."
            	ptype="STRING"/>
        <ACTION>lua clishCfgObjAdd ("vlanMembership","vlan.vlanIds2", "${vlanIds2}")
        </ACTION>
     </COMMAND>

</VIEW> 

