<?xml version="1.0" encoding="UTF-8"?>
<CLISH_MODULE xmlns="http://clish.sourceforge.net/XMLSchema" 
			  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
			  xsi:schemaLocation="http://clish.sourceforge.net/XMLSchema
              http://clish.sourceforge.net/XMLSchema/clish.xsd">
              
<!--=======================================================-->
<VIEW name="security-vpn-passthrough-config" prompt="security-config[vpn-passthrough]> "> 
    <COMMAND name="save" help="Save VPN Passthrough configuration changes.">
	     <ACTION>lua clishCfgObjExec("AttackChecks")
	     </ACTION>
    </COMMAND>
    <COMMAND name="exit" 
	     help="Save VPN Passthrough configuration changes and exit current mode." 
	     view="root-view" viewid="">
	     <ACTION>lua clishCfgObjExec("AttackChecks")</ACTION>
    </COMMAND>
    <COMMAND name="cancel" help="Roll back VPN Passthrough configuration changes.">
	     <ACTION>lua clishCfgObjReset()</ACTION>
     </COMMAND>
    
    <COMMAND name="ipsec_enable" help="Enable or Disable IPSEC Passthrough.">
	<PARAM name="ipsec"
         help="Enable / Disable IPSec Passthrough"    
	       ptype="CHOICE_INT"/>
	<ACTION>lua clishCfgObjAdd ("AttackChecks", "AttackChecks.IpsecPassthrough", "${ipsec}")
	</ACTION>
    </COMMAND>
    <COMMAND name="pptp_enable" help="Enable or Disable PPTP Passthrough.">
	<PARAM name="pptp"
         help="Enable / Disable PPTP Passthrough"    
	       ptype="CHOICE_INT"/>
	<ACTION>lua clishCfgObjAdd ("AttackChecks", "AttackChecks.PptpPassthrough", "${pptp}")
	</ACTION>
    </COMMAND>
    <COMMAND name="l2tp_enable" help="Enable or Disable L2TP Passthrough.">
	<PARAM name="l2tp"
         help="Enable / Disable L2TP Passthrough"    
	       ptype="CHOICE_INT"/>
	<ACTION>lua clishCfgObjAdd ("AttackChecks", "AttackChecks.L2tpPassthrough", "${l2tp}")
	</ACTION>
    </COMMAND>
</VIEW>
<!--=======================================================-->
    <COMMAND name="security firewall vpn_passthrough" help="VPN Passthrough setup."/>
    <!--=======================================================-->
    <COMMAND name="security firewall vpn_passthrough configure"
             help="VPN Passthrough configuration mode."
             view="security-vpn-passthrough-config" viewid="">
                      <ACTION>lua clishCfgObjNew ("AttackChecks", 
                              vpnpassthroughCfgInit, vpnpassthroughCfgSave, NULL)
                   </ACTION>
    </COMMAND>
    <!--=======================================================-->

</CLISH_MODULE>
