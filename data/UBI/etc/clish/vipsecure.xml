<VIEW name="vpn-policy-config" prompt="vpn-config[policy]> "> 
    <COMMAND name="save" help="Save vpn policy configuration changes.">
        <ACTION>lua clishCfgObjExec("vpn")</ACTION>
    </COMMAND>
    <COMMAND name="cancel" help="Roll back vpn policy configuration changes.">
        <ACTION>lua clishCfgObjReset()</ACTION>
    </COMMAND>
    <COMMAND name="exit" 
             help="Save vpn policy configuration changes and exit current mode."
             view="root-view" viewid="">
        <ACTION>lua clishCfgObjExec("vpn")</ACTION>
    </COMMAND>
