<VIEW name="net-SessionLimiting-config" prompt="net-SessionLimiting-config-profile]> "> 
    <COMMAND name="save" help="Save Session Limiting configuration changes.">
	     <ACTION>lua clishCfgObjExec("sessionLimit")
	     </ACTION>
    </COMMAND>
    <COMMAND name="exit" 
	     help="Save Session Limiting configuration changes and exit current mode." 
	     view="root-view" viewid="">
	     <ACTION>lua clishCfgObjExec("sessionLimit")</ACTION>
    </COMMAND>
    <COMMAND name="cancel" help="Roll back Session Limiting ACL configuration changes.">
	     <ACTION>lua clishCfgObjReset()</ACTION>
    </COMMAND>
<COMMAND name="profile_name"
             help="Session Limiting profile name">
             <PARAM name="ProfileName"
                    help="Session Limiting profile name"
                    ptype="STRING"/>
             <ACTION>lua clishCfgObjAdd ("sessionLimit", "sessionLimit.ProfileName", "${ProfileName}")              
             </ACTION>
    </COMMAND>     
    <COMMAND name="source_type"
             help="Type of the Session Limiting">
             <PARAM name="SourceType"
                    help="Select Session Limiting type Single Address/Address Range/Interface"
                    ptype="SESSION_LIMITING_SOURCE_TYPE"/>
             <ACTION>lua clishCfgObjAdd ("sessionLimit", "sessionLimit.SourceType", "${SourceType}")              
             </ACTION>
         </COMMAND>
    <COMMAND name="ip_or_start_address"
             help="IP of the Source user">
             <PARAM name="StartIPAddress"
                    help="IP of the Source User"
                    ptype="IP_ADDR"/>
             <ACTION>lua clishCfgObjAdd ("sessionLimit", "sessionLimit.StartIPAddress", "${StartIPAddress}")              
             </ACTION>
    </COMMAND>
    <COMMAND name="end_address"
             help="IP of the Source user">
             <PARAM name="EndIPAddress"
                    help="IP of the Source User"
                    ptype="IP_ADDR"/>
             <ACTION>lua clishCfgObjAdd ("sessionLimit", "sessionLimit.EndIPAddress", "${EndIPAddress}")              
             </ACTION>
    </COMMAND>
<COMMAND name="interface_name"
             help="show net vlan configuration for available lan interfaces">
             <PARAM name="Interface"
                    help="show net vlan configuration for available lan interfaces"
                    ptype="STRING"/>
             <ACTION>lua clishCfgObjAdd ("sessionLimit", "sessionLimit.Interface", "${Interface}")              
             </ACTION>
         </COMMAND> 
  <COMMAND name="Maximum_sessions"
             help="Enter maximum sessions to be allowed on profile configured">
             <PARAM name="MaximumSessions"
                    help="Enter maximum sessions to be allowed on profile configured"
                    ptype="MAXIMUM_SESSION"/>
             <ACTION>lua clishCfgObjAdd ("sessionLimit", "sessionLimit.MaximumSessions", "${MaximumSessions}")              
             </ACTION>
         </COMMAND> 
   <COMMAND name="schedule"
             help="Enable/Disable schedule">
             <PARAM name="EnableSchedules"
                    help="schedule"
                    ptype="CHOICE_INT"/>
             <ACTION>lua clishCfgObjAdd ("sessionLimit", "sessionLimit.EnableSchedules", "${EnableSchedules}")              
             </ACTION>
         </COMMAND> 
   <COMMAND name="schedule_name"
             help="show security schedules setup for available schedule profiles">
             <PARAM name="ScheduleProfile"
                    help="show security schedules setup for available schedule profiles"
                    ptype="STRING"/>
             <ACTION>lua clishCfgObjAdd ("sessionLimit", "sessionLimit.ScheduleProfile", "${ScheduleProfile}")              
             </ACTION>
    </COMMAND> 
</VIEW>
