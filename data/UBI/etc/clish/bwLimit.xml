    <COMMAND name="net bandwidth" help="."/> 
    <COMMAND name="net bandwidth profile" 
             help="It gives options to add/edit/delete a bandwidth profile."/>
<!--=======================================================-->
    <COMMAND name="net bandwidth profile enable" 
             help="It allows to enable/disable bandwidth profiles.">
             <PARAM name="enable"
                 help="Status of Bandwidth Profile functionality"
                 ptype="CHOICE_INT"/>
            <ACTION>lua clishCfgObjNew ("bwProfileStatus", 
                                       bwProfileStatInit, 
                                       bwProfileStatSave,
                                       bwProfileStatInputVal,
                                       "${enable}")
            clishCfgObjExec ("bwProfileStatus")
	    </ACTION>
     </COMMAND>
<!--=======================================================-->
    <COMMAND name="net bandwidth profile add" 
             help="It allows to add a bandwidth profile."
             view="net-bandwidth-profile-config" viewid="">
             <ACTION>lua clishCfgObjNew ("bwProfile", bwProfileCfgInit, 
                                        bwProfileCfgSave, 
                                        bwProfileCfgInputVal)
            </ACTION>
    </COMMAND>
<!--=======================================================-->
    <COMMAND name="net bandwidth profile edit" 
             help="It allows to edit a bandwidth profile."
             view="net-bandwidth-profile-config" viewid="">
             <PARAM name="row_id" 
	       help="Row Id Of the profile to be edited"
	       ptype="UINT"/>
             <ACTION>lua clishCfgObjNew ("bwProfile", bwProfileCfgInit, 
                                        bwProfileCfgSave, 
                                        bwProfileCfgInputVal,
                                        "${row_id}")
            </ACTION>
    </COMMAND>
<!--=======================================================-->
    <COMMAND name="net bandwidth profile delete" 
             help="It allows to delete a bandwidth profile.">
             <PARAM name="row_id" 
	       help="Row Id Of the profile to be deleted"
	       ptype="UINT"/>
             <ACTION>lua bwProfileCfgDel("${row_id}")
            </ACTION>
        </COMMAND>
<!--=======================================================-->
    <COMMAND name="net bandwidth session_limiting"
        help="It gives options to add/edit/delete a session limiting profile to allows user to limit number of sessions through the device per an IP/range of IP's/interface."/>
<!--=======================================================-->
    <COMMAND name="net bandwidth session_limiting modify_warningMessage" 
             help="It allows to edit Default warning Message."
