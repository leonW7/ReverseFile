    <COMMAND name="net bandwidth session_limiting Profiles_deleteAll" 
        help="Delete all the Session Limiting profiles.">
             <ACTION>lua clishCmdExec (sessionLimitCfgDelall)
            </ACTION>
        </COMMAND>       
