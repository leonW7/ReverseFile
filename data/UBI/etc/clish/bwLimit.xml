<VIEW name="net-bandwidth-profile-config" prompt="net-config[bandwidth-profile]> "> 
    <COMMAND name="save" help="Save bandwidth profile configuration changes.">
	<ACTION>lua clishCfgObjExec("bwProfile")</ACTION>
    </COMMAND>
    <COMMAND name="exit" 
	     help="Save bandwidth Profile configuration changes and exit current mode."
	     view="root-view" viewid="">
        <ACTION>lua clishCfgObjExec("bwProfile")</ACTION>
    </COMMAND>
    <COMMAND name="cancel" help="Roll back bandwidth Profile configuration changes.">
        <ACTION>lua clishCfgObjReset()</ACTION>
    </COMMAND>
    <COMMAND name="name" help="Unique Profile Name.">
	<PARAM name="ProfileName" 
        help="Unique Profile Name."
	ptype="STRING_ALPHA_NUM"/>
	<ACTION>lua clishCfgObjAdd ("bwProfile", "BandWidthProfile.ProfileName","${ProfileName}")
	</ACTION>
    </COMMAND>
    <COMMAND name="policytype" help="Policy Type, either Inbound or Outbound.">
	<PARAM name="Policy" 
        help="Policy Type."
	ptype="BANDWIDTH_POLICY_TYPE"/>
	<ACTION>lua clishCfgObjAdd ("bwProfile", "BandWidthProfile.Policy","${Policy}")
    </ACTION> 
   </COMMAND>
    <COMMAND name="Profiletype" help="Profile Type, either Priority or Rate.">
	<PARAM name="ProfileType" 
        help="Profile Type."
	ptype="BANDWIDTH_PROFILE_TYPE"/>
	<ACTION>lua clishCfgObjAdd ("bwProfile", "BandWidthProfile.ProfileType","${ProfileType}")
	</ACTION>
    </COMMAND>
    <COMMAND name="priority" help="Priority.">
	<PARAM name="Priority" 
        help="Priority."
	ptype="BANDWIDTH_PRIORITY_TYPE"/>
	<ACTION>lua clishCfgObjAdd ("bwProfile", "BandWidthProfile.Priority","${Priority}")
	</ACTION>
    </COMMAND>
    <COMMAND name="minimum_rate" help="Minimum Bandwidth provided by user.">
	<PARAM name="MinimumRate" 
        help="Minimum Bandwidth provided by user."
	ptype="MIN_BW_RATE"/>
	<ACTION>lua clishCfgObjAdd ("bwProfile", "BandWidthProfile.MinimumRate","${MinimumRate}")
	</ACTION>
    </COMMAND>
    <COMMAND name="maximum_rate" help="Maximum Bandwidth provided by user.">
	<PARAM name="MaximumRate" 
        help="Maximum Bandwidth provided by user."
	ptype="MAX_BW_RATE"/>
	<ACTION>lua clishCfgObjAdd ("bwProfile", "BandWidthProfile.MaximumRate","${MaximumRate}")
	</ACTION>
    </COMMAND>
    <COMMAND name="Interface" help="Enter WAN interface(WAN1/WAN2) for Outbund Policy and LAN/VLAN
           (Use show net bandwidth profile interface_list command to see list)interface for Inbound Bandwidth Profile">
	<PARAM name="WANID" 
        help="Enter WAN interface(WAN1/WAN2) for Outbund Policy and LAN/VLAN (Use show net bandwidth profile interface_list command to see list) interface for Inbound Bandwidth Profile"
	ptype="STRING"/>
	<ACTION>lua clishCfgObjAdd ("bwProfile", "BandWidthProfile.WANID","${WANID}")
	</ACTION>
    </COMMAND>

</VIEW>
