    </COMMAND>
    <COMMAND name="cancel" help="Roll back subjectList configuration changes.">
	     <ACTION>lua clishCfgObjReset()</ACTION>
     </COMMAND>
     <COMMAND name="subject" help="Subject in the Mail_Id">
	<PARAM name="Subject" 
        help="Enter the Subject"
	       ptype="STRING"/>
	<ACTION>lua clishCfgObjAdd ("subjectList", "subjectList.subject", 
		    "${Subject}")
        </ACTION>
    </COMMAND>
    <COMMAND name="mail_Id" help="Enter the Mail_Id">
	<PARAM name="MailId" 
        help="Enter the Mail_Id"
	       ptype="STRING"/>
	<ACTION>lua clishCfgObjAdd ("subjectList", "subjectList.mailId", 
		    "${MailId}")
        </ACTION>
    </COMMAND>
    <COMMAND name="action" help="Action Allow/Block">
	<PARAM name="Action" 
        help="Enter the action to be Taken"
	       ptype="FIREWALL_POLICY"/>
	<ACTION>lua clishCfgObjAdd ("subjectList", "subjectList.action", 
		    "${Action}")
        </ACTION>
    </COMMAND>
  </VIEW>
