<VIEW name="security-url-filtering-ACL-config" prompt="security-config[url-filtering-ACL]> "> 
    <COMMAND name="save" help="Save url filtering ACL configuration changes.">
	     <ACTION>lua clishCfgObjExec("urlFilteringACL")
	     </ACTION>
    </COMMAND>
    <COMMAND name="exit" 
