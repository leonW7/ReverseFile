<VIEW name="security-url-filtering-ACL-config-profile" prompt="security-config[url-filtering-ACL-profile]> "> 
    <COMMAND name="save" help="Save url filtering ACL configuration changes.">
	     <ACTION>lua clishCfgObjExec("urlFilteringACLProfile")
	     </ACTION>
    </COMMAND>
    <COMMAND name="exit" 
	     help="Save url filtering ACL configuration changes and exit current mode." 
	     view="root-view" viewid="">
	     <ACTION>lua clishCfgObjExec("urlFilteringACLProfile")</ACTION>
    </COMMAND>
    <COMMAND name="cancel" help="Roll back url filtering ACL configuration changes.">
	     <ACTION>lua clishCfgObjReset()</ACTION>
    </COMMAND>
<COMMAND name="policy_name"
             help="url filtering acl profile name">
             <PARAM name="policyName"
                    help="url filtering acl profile name"
                    ptype="STRING"/>
             <ACTION>lua clishCfgObjAdd ("urlFilteringACLProfile", "urlFilteringACLPolicy.policyName", "${policyName}")              
             </ACTION>
    </COMMAND>     
    <COMMAND name="policy_type"
             help="Type of the url filtering acl">
             <PARAM name="policyType"
                    help="Select url filtering acl type Single Address/Address Range/Interface"
                    ptype="URL_Filtering_ACL_TYPE"/>
             <ACTION>lua clishCfgObjAdd ("urlFilteringACLProfile", "urlFilteringACLPolicy.policyType", "${policyType}")              
             </ACTION>
         </COMMAND>
    <COMMAND name="ip_address"
             help="IP of the Source user">
             <PARAM name="ipAddress"
                    help="IP of the Source User"
                    ptype="IP_ADDR"/>
             <ACTION>lua clishCfgObjAdd ("urlFilteringACLProfile", "urlFilteringACLPolicy.ipAddress", "${ipAddress}")              
             </ACTION>
    </COMMAND>
    <COMMAND name="start_address"
             help="IP of the Source user">
             <PARAM name="startIpAddress"
                    help="IP of the Source User"
                    ptype="IP_ADDR"/>
             <ACTION>lua clishCfgObjAdd ("urlFilteringACLProfile", "urlFilteringACLPolicy.startIpAddress", "${startIpAddress}")              
             </ACTION>
    </COMMAND>
    <COMMAND name="end_address"
             help="IP of the Source user">
             <PARAM name="endIpAddress"
                    help="IP of the Source User"
                    ptype="IP_ADDR"/>
             <ACTION>lua clishCfgObjAdd ("urlFilteringACLProfile", "urlFilteringACLPolicy.endIpAddress", "${endIpAddress}")              
             </ACTION>
    </COMMAND>
<COMMAND name="interface_name"
             help="show net vlan configuration for available lan interfaces">
             <PARAM name="interfaceName"
                    help="show net vlan configuration for available lan interfaces"
                    ptype="STRING"/>
             <ACTION>lua clishCfgObjAdd ("urlFilteringACLProfile", "urlFilteringACLPolicy.interfaceName", "${interfaceName}")              
             </ACTION>
    </COMMAND> 
</VIEW>
