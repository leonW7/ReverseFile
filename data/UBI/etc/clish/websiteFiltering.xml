    <COMMAND name="security website_filter url_Filtering_ACL" help="url filtering ACL configuration setup."/>
    <!--=======================================================-->
    <COMMAND name="security website_filter content_filtering configure"
             help="content filtering configuration mode."
             view="security-content-filtering-config" viewid="">
                   <ACTION>lua clishCfgObjNew ("contentFiltering", 
                              contentFilteringCfgInit, contentFilteringCfgSave, 
				       contentFilteringCfgInputVal)
                   </ACTION>
    </COMMAND>
    <!--=======================================================-->
    <COMMAND name="security website_filter url_Filtering_ACL configure"
             help="url filtering ACL configuration mode."
             view="security-url-filtering-ACL-config" viewid="">
                   <ACTION>lua clishCfgObjNew ("urlFilteringACL", 
                              urlFilteringACLCfgInit, urlFilteringACLCfgSave, 
				       urlFilteringACLCfgInputVal)
                   </ACTION>
               </COMMAND>
<!--=======================================================-->
    <COMMAND name="security website_filter url_Filtering_ACL add"
             help="url filtering ACL profile configuration mode."
             view="security-url-filtering-ACL-config-profile" viewid="">
                   <ACTION>lua clishCfgObjNew ("urlFilteringACLProfile", 
                              urlFilteringACLProfileCfgInit, urlFilteringACLProfileCfgSave, 
				       urlFilteringACLProfileCfgInputVal)
                   </ACTION>
    </COMMAND>
    <COMMAND name="security website_filter url_Filtering_ACL edit"
             help="url filtering ACL profile configuration mode."
             view="security-url-filtering-ACL-config-profile" viewid="">
	<PARAM name="row_id" 
	       help=" Row Id Of the domain to be edited"
	       ptype="UINT"/>
                   <ACTION>lua clishCfgObjNew ("urlFilteringACLProfile", 
                              urlFilteringACLProfileCfgInit, urlFilteringACLProfileCfgSave, 
				       urlFilteringACLProfileCfgInputVal,"${row_id}")
                   </ACTION>
    </COMMAND>
    <COMMAND name="security website_filter url_Filtering_ACL delete"
             help="url filtering ACL profile configuration mode.">
	<PARAM name="row_id" 
	       help="Row Id Of the domain to be deleted"
	       ptype="UINT"/>
                   <ACTION>lua clishCmdExec(urlFilteringACLProfileCfgDel,"${row_id}")
                   </ACTION>
    </COMMAND>
    <COMMAND name="security website_filter url_Filtering_ACL deleteAll"
        help="Delete all the url Filtering ACL profiles.">
        <ACTION>lua clishCmdExec(urlFilteringACLProfileCfgDelall)
	    </ACTION>
    </COMMAND>
