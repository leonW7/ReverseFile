<?xml version="1.0" encoding="UTF-8"?>
<CLISH_MODULE xmlns="http://clish.sourceforge.net/XMLSchema" 
			  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
			  xsi:schemaLocation="http://clish.sourceforge.net/XMLSchema
              http://clish.sourceforge.net/XMLSchema/clish.xsd">
              
<!--=======================================================-->
<VIEW name="security-content-filtering-config" prompt="security-config[content-filtering]> "> 
    <COMMAND name="save" help="Save contentFiltering configuration changes.">
	     <ACTION>lua clishCfgObjExec("contentFiltering")
	     </ACTION>
    </COMMAND>
    <COMMAND name="exit" 
	     help="Save content Filtering configuration changes and exit current mode." 
	     view="root-view" viewid="">
	     <ACTION>lua clishCfgObjExec("contentFiltering")</ACTION>
    </COMMAND>
    <COMMAND name="cancel" help="Roll back content filtering configuration changes.">
	     <ACTION>lua clishCfgObjReset()</ACTION>
    </COMMAND>
    <COMMAND name="content_filtering"
             help="Enable/Disable content Filtering">
             <PARAM name="status"
                    help="Enable/Disable content Filtering"
                    ptype="CHOICE_INT"/>
             <ACTION>lua clishCfgObjAdd ("contentFiltering", "ContentFiltering.Status", "${status}")              
             </ACTION>
    </COMMAND>
    <COMMAND name="proxy_enable"
             help="enable/disable proxy">
             <PARAM name="proxy"
                    help="enable/disable proxy"
                    ptype="CHOICE_INT"/>
             <ACTION>lua clishCfgObjAdd ("contentFiltering", "WebComponents.ProxyEnable", "${proxy}")              
             </ACTION>
    </COMMAND>
    <COMMAND name="java_enable"
             help="enable/disable java">
             <PARAM name="java"
                    help="enable/disable java"
                    ptype="CHOICE_INT"/>
             <ACTION>lua clishCfgObjAdd ("contentFiltering", "WebComponents.JavaComponentEnable", "${java}")              
             </ACTION>
    </COMMAND>
    <COMMAND name="activex_enable"
             help="enable/disable activex">
             <PARAM name="activex"
                    help="enable/disable activex"
                    ptype="CHOICE_INT"/>
             <ACTION>lua clishCfgObjAdd ("contentFiltering", "WebComponents.ActivexControlEnable", "${activex}")              
             </ACTION>
    </COMMAND>
    <COMMAND name="cookies_enable"
             help="enable/disable cookies">
             <PARAM name="cookies"
                    help="enable/disable cookies"
                    ptype="CHOICE_INT"/>
             <ACTION>lua clishCfgObjAdd ("contentFiltering", "WebComponents.CookiesEnable", "${cookies}")              
             </ACTION>
    </COMMAND>
</VIEW>
