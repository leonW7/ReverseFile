<VIEW name="security-approved-urls-config" prompt="security-config[approved-urls]> "> 
    <COMMAND name="save" help="Save trusted domains configuration changes.">
	     <ACTION>lua clishCfgObjExec("trustedDomains")
	     </ACTION>
    </COMMAND>
    <COMMAND name="exit" 
	     help="Save trusted domains configuration changes and exit current mode." 
	     view="root-view" viewid="">
	     <ACTION>lua clishCfgObjExec("trustedDomains")</ACTION>
    </COMMAND>
    <COMMAND name="cancel" help="Roll back trusted domains configuration changes.">
	     <ACTION>lua clishCfgObjReset()</ACTION>
    </COMMAND>
    <COMMAND name="url"
             help="trusted domain name">
             <PARAM name="domain"
                    help="trusted domain name"
                    ptype="STRING"/>
             <ACTION>lua clishCfgObjAdd ("trustedDomains", "TrustedDomains.KeywordToAllow", "${domain}")              
             </ACTION>
    </COMMAND>
    <COMMAND name="protocol"
             help="protocol speciifcation for trusted URL">
             <PARAM name="trusted_protocol"
                    help="protocol specification for trusted URL"
                    ptype="TRUSTED_PROTOCOL"/>
             <ACTION>lua clishCfgObjAdd ("trustedDomains", "TrustedDomains.Protocol", "${protocol}")              
             </ACTION>
    </COMMAND>
</VIEW>
